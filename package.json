{
  "name": "instant-quote",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "verify": "npm run lint && npm run typecheck && npm run test:unit && npm run build",
    "db:apply": "psql \"$SUPABASE_DB_URL\" -f sql/schema.sql -f sql/policies.sql",
    "db:seed": "tsx db/seed.ts",
    "postinstall": "node scripts/postinstall.cjs",
    "verify:view": "npm run build",
    "codemod:cookies": "node --experimental-modules scripts/codemods/next15-cookies.mjs",
    "codemod:runtime": "node scripts/codemods/add-node-runtime.mjs"
  },
  "dependencies": {
    "@react-three/fiber": "^8.13.5",
    "@supabase/ssr": "latest",
    "@supabase/supabase-js": "^2.39.5",
    "@tanstack/react-table": "^8.8.7",
    "date-fns": "^2.30.0",
    "dxf-parser": "^1.1.2",
    "lucide-react": "^0.323.0",
    "next": "latest",
    "occt-import-js": "^0.0.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.2",
    "three": "^0.179.1",
    "three-mesh-bvh": "^0.9.1",
    "three-stdlib": "^2.36.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/config-array": "latest",
    "@eslint/object-schema": "latest",
    "@playwright/test": "latest",
    "@types/node": "latest",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.17",
    "@types/three": "^0.179.0",
    "autoprefixer": "^10.4.16",
    "eslint": "latest",
    "eslint-config-next": "14.1.0",
    "glob": "^10.3.10",
    "jsdom": "latest",
    "postcss": "^8.4.31",
    "rimraf": "latest",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.5",
    "typescript": "^5.4.2",
    "vitest": "latest"
  },
  "engines": {
    "node": ">=22 <23"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals"
    ]
  },
  "overrides": {
    "cookie": "^0.7.2"
  }
  // package.json
{
  "scripts": {
    "doctor": "node scripts/diagnostics.mjs",
    "verify:strict": "npm run lint && npm run typecheck && npm run test:unit && next build --no-lint --show-error-details"
  }

}
